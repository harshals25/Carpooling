<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="pool.dto">
    <class name="Apply" table="employeerequest25">
        <id name="serialNo" column="serialno">
            <generator class="native"/>
        </id>
    <property name="empId" column="employeeid"/>
    <property name="post" column="post"/>
    <property name="salary" column="salary"/>
    <property name="dateOfApply" column="dateofapplyu"/>
    <property name="carType" column="cartype"/>
    <property name="location" column="location"/>
    <property name="status" column="employeeStatus"/>
    </class>
</hibernate-mapping>
